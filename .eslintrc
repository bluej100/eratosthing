{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "global-strict": [
      2,
      "always"
    ],
    "no-constant-condition": 0
  },
  "globals": {
  },
  "env": {
    "es6": true,
    "node": true
  }
}
